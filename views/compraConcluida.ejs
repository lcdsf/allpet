            </div>

            <div id="detalhescompra">
                <p>DETALHES DA COMPRA - ID: <%=compra.id%></p>
                <p>Valor: R$ <%=valortotal.toFixed(2).replace('.', ',')%> - <%=compra.forma_pgto%></p>
                
                <div id="contdetcompra">

                    <% if (typeof itenscompra !== 'undefined'){ %>
                        <% itenscompra.forEach(item => { %>    
                            <div id="itemcompra">
                                <img src="/img/racao.png"> 
                                <br>
                                <label><%=item.produto.nome%></label>
                                <br>
                                <label>R$ <%=item.produto.preco.toFixed(2).replace('.', ',')%></label>
                                <br>
                                <label>Qtd: <%=item.quantidade%></label>    
                            </div>
                        <% }) %>
                    <% } %>


                </div>

            </div>

            
        </div>

        <% if (typeof endereco !== 'undefined'){%>
            <h3>Endereço para entrega</h3>
            <p style="text-align: center;">Rua <%=endereco.rua%>, nº <%=endereco.numero%>, Bairro <%=endereco.bairro%>, <%=endereco.cidade%> - <%=endereco.estado%> - CEP: <%=endereco.cep%></p>
        <% }else{ %>
            <h3>Você optou por retirar a compra na loja</h3>
        <% } %>


        <div id="btnscompraconcluida">
             <a href="/cliente/compra/<%=compra.id%>"><button>VER DETALHES</button></a> 

            <a href="/cliente/ajuda"><button>AJUDA</button></a>

